[alias]
	ap = add --patch
	au = add --update
	br = branch
	bv = branch --verbose --verbose
	ca = commit --amend
	ci = commit
	co = checkout
	cp = cherry-pick
	dc = diff --cached
	dh = diff HEAD
	di = diff
	du = diff @{u}
	ff = merge --ff-only
	fp = format-patch
	fx = commit --fixup
	lg = log --graph --decorate --oneline
	lp = log --patch
	ls = ls-files -oc --exclude-standard
	rh = reset HEAD
	rl = reflog
	rp = reset --patch
	st = status --short --branch -uno
	ss = stash save
	sp = stash pop
	sd = stash drop
	sc = stash clear
	csq = commit --squash
	rba = rebase --abort
	rbc = rebase --continue
	rbi = rebase --interactive
	rbs = rebase --skip
	top = shortlog -ns
	wip = commit -amwip
[branch]
	autosetupmerge = always
	autosetuprebase = always
[color]
	ui = auto
[core]
	editor = vim
	pager = less -S
	excludesfile = ~/.gitignore
	attributesfile = ~/.gitattributes
[credential]
	helper = osxkeychain
[diff]
	renames = copies
	tool = vimdiff
	algorithm = patience
[diff "gpg"]
	textconv = gpg2 -qd --no-tty
[diff "keyring"]
	textconv = gpg2 -v
	xfuncname = "^((pub|uid) .*)"
[diff "tar"]
	textconv = tar -tvf
[difftool]
	prompt = false
[fetch]
	prune = true
[grep]
	extendedregexp = true
[help]
	format = web
[init]
	templatedir = ~/.git_template
[instaweb]
	httpd = lighttpd -f
	local = true
	port = 8080
[interactive]
	singlekey = true
[log]
	decorate = short
[merge]
	conflictstyle = diff3
[merge "bundle"]
	name = bundle
	driver = bundle
[pager]
	log = less -+$LESS -iRSq
[push]
	default = upstream
[rebase]
	autosquash = true
[rerere]
	enabled = true
[sendemail]
	smtpencryption = tls
	smtpserver = smtp.gmail.com
	smtpserverport = 587
	smtpuser = jacknagel@gmail.com
[user]
